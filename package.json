{
	"name": "@raythurnevoid/svelte-template",
	"version": "0.0.27",
	"scripts": {
		"env": "cross-env TS_NODE_PROJECT=\"tsconfig.tsnode.json\"",
		"ts-node": "npm run env -- node -r ts-node/register -r tsconfig-paths/register --",
		"ts-node:inspect": "npm run ts-node -- --inspect --",
		"webpack": "npm run env -- webpack --",
		"dev": "npm run webpack -- serve --config webpack.config.devserver.ts --progress --open ",
		"build": "npm run webpack -- --env production",
		"watch": "npm run webpack -- --watch",
		"analyze": "npm run webpack -- --env analyzeBundle",
		"start": "sirv dist",
		"validate": "svelte-check",
		"clean-lib": "del-cli lib",
		"copy": "copyfiles ./build/**/*.* package.json tsconfig.json webpack* svelte.config.js lib",
		"tsc": "tsc -p tsconfig.lib.json",
		"lib": "npm-run-all clean-lib copy tsc",
		"prepackage": "npm run lib",
		"package": "npm publish ./lib --access public"
	},
	"browserslist": [
		"defaults and supports es6-module"
	],
	"devDependencies": {
		"autoprefixer": "^10.2.5",
		"copyfiles": "^2.4.1"
	},
	"dependencies": {
		"svelte": "^3.37.0",
		"svelte-check": "^1.4.0",
		"svelte-loader": "^3.1.0",
		"svelte-preprocess": "^4.7.0",
		"@tsconfig/svelte": "^1.0.10",
		"@types/copy-webpack-plugin": "^6.4.1",
		"@types/node": "^14.14.37",
		"@types/webpack-dev-server": "^3.11.3",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^8.1.1",
		"cross-env": "^7.0.3",
		"css-loader": "^5.2.1",
		"del-cli": "^3.0.1",
		"file-loader": "^6.2.0",
		"mini-css-extract-plugin": "^1.4.1",
		"npm-run-all": "^4.1.5",
		"sass": "^1.32.8",
		"sass-loader": "^11.0.1",
		"sirv-cli": "^1.0.11",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.1.0",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"tsconfig-paths-webpack-plugin": "^3.5.1",
		"tslib": "^2.2.0",
		"typescript": "^4.2.4",
		"webpack": "^5.31.2",
		"webpack-bundle-analyzer": "^4.4.0",
		"webpack-cli": "^4.6.0",
		"webpack-dev-server": "^3.11.2"
	}
}
